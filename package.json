  "scripts": {
    "start": "react-native start --config $PWD/rn-cli.config.js",
    "android": "react-native run-android --config $PWD/rn-cli.config.js",
    "ios": "react-native run-ios --config $PWD/rn-cli.config.js",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/coinsane/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "postinstall": "react-native link",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "clear-cache": "gulp clear-cache",
    "storybook": "storybook start -p 7007 --metro-config $PWD/rn-cli.config.js",
    "prestorybook": "rnstl",
    "lint": "tslint -p $PWD/tsconfig.json"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": "./src",
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-eslint": "^10.0.0",
    "babel-polyfill": "^6.26.0",
    "d3-shape": "^1.2.2",
    "eslint": "^5.6.0",
