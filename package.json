    "react-native-quick-actions": "^0.3.7",
    "react-native-router-flux": "^4.0.3",
    "react-native-simple-markdown": "^1.1.0",
    "react-native-spinkit": "^1.1.1",
    "react-native-splash-screen": "^3.1.1",
    "react-native-store-review": "^0.1.5",
    "react-native-svg": "^7.0.3",
    "react-native-svg-charts": "^5.2.0",
    "react-native-svg-icon": "^0.8.1",
    "react-native-swiper": "^1.5.13",
    "react-native-switch-pro": "^0.4.8",
    "react-native-switch-selector": "^1.1.8",
    "react-native-version-number": "^0.3.4",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "tslib": "^1.9.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
